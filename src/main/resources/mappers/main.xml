<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTO Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="common">
    <select id="selectUsers" resultType="hashmap">
        SELECT USER_ID
             , USER_PW
             , USER_NAME
             , USER_GENDER
             , USER_AGE
             , USER_EMAIL
          FROM USERS
    </select>
</mapper>